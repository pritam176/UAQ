<adapter-config name="GetLookUpsFrmDB" adapter="Database Adapter" wsdlLocation="GetLookUpsFrmDB.wsdl" xmlns="http://platform.integration.oracle/blocks/adapter/fw/metadata">
  
  <connection-factory location="eis/uaqsoa_db" UIConnectionName="DevDB" adapterRef=""/>
  <endpoint-interaction portType="GetLookUpsFrmDB_ptt" operation="GetLookUpsFrmDB">
    <interaction-spec className="oracle.tip.adapter.db.DBPureSQLInteractionSpec">
      <property name="SqlString" value="select ownr.nationalityname_en ownernatEng, ownr.nationalityname_ar ownernatArb,  ownr.nationality_id ownernatId,  ser.nationalityname_en usernatEng, ser.nationalityname_ar usernatArb,  ser.nationality_id usernatId, emi.emiratename_en emirateEng,  emi.emiratename_ar emirateArb,  emi.emirate_id emirateid, typeofuserEngName, typeofuserArabicname, sourcetypeEngName, sourcetypeArabicname, apptypeEngname, apptypeArbname  from (select owner.nationalityname_en,owner.nationalityname_ar,owner.nationality_id  from nationality_lookups owner where owner.nationality_id=#ownernationality) ownr full outer join   (select usr.nationalityname_en,usr.nationalityname_ar,usr.nationality_id from  nationality_lookups usr where usr.nationality_id=#usrnationality) ser on 1=1 full outer join  (select emirate.emiratename_en ,emirate.emiratename_ar ,emirate.emirate_id from emirate_lookups emirate where emirate.emirate_id=#emirate_id) emi on 1=1 full outer join  (SELECT DECODE (#typeofuserid1, 1, 'Individual', 2, 'Establishment',  'N/A')     typeofuserEngName, DECODE (#typeofuserid2, 1, 'شخص',                         2, 'مؤسسة',                                   'N/A')         typeofuserArabicname                          FROM dual) typeofusr  on 1=1 full outer join (SELECT DECODE (#sourcetype1, 1,'On Line',                                  2, 'Front Desk',                                  'N/A')         sourcetypeEngName , DECODE (#sourcetype2, 1, 'عبر الانترنت',                     2, 'مكتب استقبال',                        'N/A')         sourcetypeArabicname FROM dual) srctype on 1=1 full outer join (SELECT DECODE (#apptype1, 1,'UAE Citizen',                               2, 'UAE Resident',                                                             3,'GCC Citizen',                               4,'GCC Resident',                               5,'Visitor',                                    'N/A')         apptypeEngname, DECODE (#apptype2, 1, 'الإمارات العربية المتحدة مواطن',                      2, 'الإمارات العربية المتحدة المقيم',                      3,'مواطن دول مجلس التعاون الخليجي',                        4,'المقيم دول مجلس التعاون الخليجي',                       5,'زائر',                                          'N/A')         apptypeArbname FROM dual) apptype on 1=1"/>
      <property name="GetActiveUnitOfWork" value="false"/>
    </interaction-spec>
    <input/>
    <output/>
  </endpoint-interaction>

</adapter-config>