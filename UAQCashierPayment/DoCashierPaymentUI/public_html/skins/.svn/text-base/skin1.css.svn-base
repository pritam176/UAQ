/**
 * -------------------------------
 * Buttons
 * -------------------------------
 */

.AFButtonNative:alias
{
  line-height: 18px;
  padding: 0 10px 1px;  
}

/* enabled */
.AFButtonForeground:alias
{
  margin: 0;
  font-size: 12px;
  color: #666666;
  text-decoration: none;
  cursor: pointer;
}

.AFButtonBackground:alias
{  
  background-color: #EBECED;                     
  background-image: none;
}

.AFButtonBorder:alias
{
  border: 1px solid #C9CCCE;                     
  border-radius: 3px;
}

af|goButton,
af|commandButton
{
  background-image: none;   
}

af|goButton.ok,
af|commandButton.ok
{
  background-color: #1466B2;
  background-image: none;
  border-color: #005C9D;
  color: #FFFFFF;
}

/* hover */
.AFButtonBackgroundHover:alias
{  
  background-color: #F1F2F2;                     
  background-image: none;   
  box-shadow: 0 1px 0 #E8E8E8;
}

.AFButtonBorderHover:alias
{
  border-color: #A9ABAE;                 
}

af|goButton.ok:hover,
af|commandButton.ok:hover
{
  background-color: #2A87D5;
  background-image: none;
  border-color: #005897;
  color: #FFFFFF;
  box-shadow-color: #E8E8E8;
}

/* active */
.AFButtonBackgroundActive:alias
{  
  background-color: #CCCDCE;                    
  background-image: none;   
  box-shadow: 0 2px 4px #ABACAD inset;
}

.AFButtonBorderActive:alias
{
  border-color: #8F9295;
}

af|goButton.ok:active,
af|goButton.ok:active:hover,
af|goButton.ok:active:hover:focus,
af|commandButton.ok:active,
af|commandButton.ok:active:hover,
af|commandButton.ok:active:hover:focus
{
  background-color: #005C9D;
  border-color: #003264;
  color: #FFFFFF;
  box-shadow: 0 2px 4px #004D84 inset;
}

/* disabled */
.AFButtonBackgroundDisabled:alias
{
  background-color: #EBECED;
  color: #666666;
  box-shadow: none;
  opacity: 0.5;
}

.AFButtonBorderDisabled:alias
{
  border-color: #C9CCCE;
}

af|goButton.ok:disabled,
af|commandButton.ok:disabled
{
  -tr-rule-ref: selector(".AFButtonBackgroundDisabled:alias");
}

/* IE 7 and 8 exceptions */
@agent ie and (version: 7), ie and (version: 8)
{
  .AFButtonBackgroundHover:alias
  {  
    -tr-inhibit: box-shadow;
  }

  .AFButtonBackgroundActive:alias
  {  
    -tr-inhibit: box-shadow;
  }

  .AFButtonBackgroundDisabled:alias
  {  
    -tr-inhibit: box-shadow;
  }

  .AFButtonBorder:alias
  {  
    -tr-inhibit: border-radius;
  }
}

@agent (touchScreen:none)
{
  /* workaround adf faces bug 14239220 (remove when fixed) */
  af|commandToolbarButton:hover,
  af|commandToolbarButton:text-only:hover,
  af|toolbar af|commandToolbarButton:hover,
  af|toolbar af|commandToolbarButton:text-only:hover
  {
    -tr-rule-ref: selector(".AFButtonBackgroundHover:alias");
  }
}

