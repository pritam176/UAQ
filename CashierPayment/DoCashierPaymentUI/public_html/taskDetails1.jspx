      <af:resource type="javascript">
 
       var actionbtn;
       var response_Json

 function submitActions( amountV,  ecrIdNoV,  serviceCodeV, transactionTypeV) {
   //alert('submitting actions');
   document.getElementById("xx1::content").value=amountV;
  document.getElementById("xx2::content").value=ecrIdNoV;
   document.getElementById("xx3::content").value=serviceCodeV;
   document.getElementById("xx4::content").value=transactionTypeV;
  // alert('start calling');
            $.ajax( {
                url : "http://localhost:8080/uaqwsfd/pos/doTransaction/",
                data :JSON.stringify({amount:amountV,ecrIdNo:ecrIdNoV,serviceCode:serviceCodeV,transactionType:transactionTypeV}),
                contentType : "application/json", type : "POST", async : false,
                beforeSend : function (req) {
                  //   alert('before send');
                    //console.log('before send');
                    req.setRequestHeader("Authorization", "Basic cG9zOnBvcw==");
                },
                error : function (XMLHttpRequest, textStatus, errorThrown) {
                //    alert('error');
                    ajaxError(XMLHttpRequest, textStatus, errorThrown);
                },
                success : function (data) {
              //   alert('success');
                    //console.log('success');
                     response_Json = JSON.stringify(data);
                   // alert(response_Json);
                     //console.log(response_Json);
                    // alert(actionbtn);
                    $('.hiddenPOS')[0].click();
                    //AdfCustomEvent.queue(actionbtn.getSource(), "servListener2", {fvalue : response_Json}, true);                
                    return false;
                },
                error : function (xhr, status, err) {
                //    alert('error');
                     console.log('error');
                    var err = eval('(' + xhr.responseText + ')');
                 //   alert('' + err.Message);
                    console.log('Error in Ajax call:' + err.Message);
                }
            });
        }
function clicked(newbtn) {
  //  alert('clicked calling server listner');
  //  alert(response_Json);
  //  alert(newbtn);
    AdfCustomEvent.queue(newbtn.getSource(), "servListener2", {fvalue : response_Json}, true);
}

        
function cliListener(actionEvent) {
  
     actionbtn=actionEvent.getSource();
    // alert(actionbtn);
    
        $.ajax( {
                url : "http://localhost:8080/uaqwsfd/pos/getPosInfo/",
                data : '', contentType : "application/json", type : "GET", async : false
                ,beforeSend : function (req) {
                 //   alert('before send');
                    console.log('before send');
                    req.setRequestHeader("Authorization", "Basic cG9zOnBvcw==");
                },
                error : function (XMLHttpRequest, textStatus, errorThrown) {
                 //   alert('error');
                    ajaxError(XMLHttpRequest, textStatus, errorThrown);
                },
                success : function (data) {
                //  alert('success');
                    document.getElementById('xx6::content').value = data.merchantID;
                    document.getElementById('xx5::content').value = data.terminalID;
                    response_Json = JSON.stringify(data);
                    //alert(response_Json);
                     console.log(response_Json);
                    AdfCustomEvent.queue(actionEvent.getSource(), "servListener", {fvalue : response_Json}, true);

                    return false;
                },
                error : function (xhr, status, err) {
                   // alert('error');
                    console.log('error');
                    var err = eval("(" + xhr.responseText + ")");
                    alert('' + err.Message);
                    console.log('Error in Ajax call:' + err.Message);
                }
            });
        
    
    }
