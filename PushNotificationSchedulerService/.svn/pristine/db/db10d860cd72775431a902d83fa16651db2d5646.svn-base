package com.uaq.pn.exception;

/**
 * @author nsharma
 * 
 */
public class UAQFaultCode {

	public static final UAQFaultCode USER_NAME_PK = new UAQFaultCode(
			"duplicate.user");

	private String errorCode = null;

	/**
	 * @param errorCode
	 */
	private UAQFaultCode(String errorCode) {
		this.errorCode = errorCode;
	}

	/*
	 * returns the error code.
	 * 
	 * @see java.lang.Object#toString()
	 */
	public String toString() {
		return this.errorCode;
	}

}
